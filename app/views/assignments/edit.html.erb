<h1>Editing Week</h1>

<%= print_date(@week) %>

<br><br>
<% @assignments.each do |assignment| %>
  <%= render('form', assignment: assignment) %>
<% end %>


<% if !@week.fully_assigned? %>
  <div><%= link_to "Add assignment", new_office_assignment_path(current_office, week: @week), class: "btn btn-dark btn-sm" %></div>
<%end%>